# Database Configuration (OBRIGATÓRIO)
# Obtenha no Supabase: Settings → Database → Connection String (URI, não Pooler)
DATABASE_URL=postgresql://postgres:YOUR_PASSWORD@db.YOUR_PROJECT_REF.supabase.co:5432/postgres

# Flask Secret Key (OBRIGATÓRIO - gere com: openssl rand -hex 32)
SECRET_KEY=change-me-to-random-secret-key

# Admin Default User
ADMIN_USER=admin
ADMIN_PASS=admin123

# Google OAuth (opcional)
GOOGLE_CLIENT_ID=

# App Config
FLASK_DEBUG=0
PORT=5000
